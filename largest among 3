#include <stdio.h>
int main ()
{
  int a,b,c;
  printf ("Enter your integer a : ");
  scanf("%d",&a);
  printf ("Enter your integer b : ");
  scanf("%d",&b);
  printf ("Enter your integer c : ");
  scanf("%d",&c);
  
  if (a>b)
  {
    if (a>c)
    printf("%d is the largest number.",a);
    else
    printf("%d is the largest number.",c);
  }

  else if (b>c)
  {
    printf ("%d is the largest number",b);
  }
  else 
    printf ("%d is the largest number",c);

  return 0;
}
